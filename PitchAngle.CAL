[Variables]
 c1=(1540);     //Wheelbase

[PitchAngle]

PitchAngle=-(#Ride_Height_Rear,#Ride_Height_Front);
PitchAngle=/(#PitchAngle,@c1);
PitchAngle=ArcSin(#PitchAngle);
PitchAngle=*(180,#PitchAngle);
PitchAngle=/(#PitchAngle,@Pi);
PitchAngle=Set(DIM='deg');
